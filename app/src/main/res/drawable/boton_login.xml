<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false" >
        <shape
            android:shape="rectangle" >
            <corners
                android:radius="5dp"
                />
            <padding
                android:left="0dp"
                android:top="0dp"
                android:right="0dp"
                android:bottom="0dp"
                />
            <solid
                android:color="@color/azulB"
                />
            <size
                android:width="220dp"
                android:height="20dp"
                />
            <stroke
                android:width="3dp"
                android:color="@color/azulB"
                />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape
            android:shape="rectangle" >
            <corners
                android:radius="5dp"
                />
            <padding
                android:left="0dp"
                android:top="0dp"
                android:right="0dp"
                android:bottom="0dp"
                />
            <solid
                android:color="@color/azulC"
                />
            <size
                android:width="220dp"
                android:height="20dp"
                />
            <stroke
                android:width="3dp"
                android:color="@color/azulB"
                />
        </shape>
    </item>
</selector>